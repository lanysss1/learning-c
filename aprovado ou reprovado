#include <stdio.h>

int main() {
    float n1, n2, media;
    printf("Digite a primeira nota: ");
    scanf("%f", &n1);
    printf("Digite a segunda nota: ");
    scanf("%f", &n2);
    media = (n1+n2)/2;
    printf("A media final e %2.2f\n ", media);
    if (media >=7.00){
        printf("Passou! \n");
    } else {
        printf("Reprovado ");
    }
    return 0;
}
